  "scripts": {
    "test": "npm run devdown && npm run devup && npm run build && npm run jasmine ",
    "testing": "npm run testdown && npm run testup && set NODE_ENV=test && npm run build && npm run jasmine",
    "lint": "eslint . --ext .ts",
    "build": "tsc",
    "start": "nodemon src/index.ts",
    "startjs": "nodemon dist/.",
    "jasmine": "jasmine",
    "prettier": "prettier --config .prettierrc {,!(node_modules)/**/}*.ts --write",
    "devup": "db-migrate up --config ./database.json --e dev",
    "devdown": "db-migrate reset --config ./database.json --e dev",
    "testup": "set NODE_ENV=test&& db-migrate up --config ./database.json --e test",
    "testdown": "set NODE_ENV=test&& db-migrate reset --config ./database.json --e test"
  },
